{
  "name": "@uibakery/data",
  "version": "0.0.8",
  "description": "UIBakery data hooks and utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepublishOnly": "npm run build",
    "license-check:dev": "license-checker --development --summary --onlyAllow \"$(cat ../../../allowed-licenses-list-js.txt | tr '\\n' ';')\"",
    "license-check:prod": "license-checker --production --summary --onlyAllow \"$(cat ../../../allowed-licenses-list-js.txt | tr '\\n' ';')\"",
    "license-check-report:prod": "license-checker --production --relativeLicensePath --out ../../../license-report/code/uibakery-data.txt",
    "license-check": "npm run license-check:dev && npm run license-check:prod"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "UIBakery",
  "license": "ISC",
  "devDependencies": {
    "license-checker": "~25.0.1",
    "rimraf": "~3.0",
    "typescript": "~5.4.5"
  }
}
